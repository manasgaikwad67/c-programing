#include<stdio.h>
int main()
{
int i,n,max,min;
//printf("enter the number of students");
//scanf("%d",&n);
printf("enter 5 students marks out of 100");
int m[4];
for(i=0;i<=4;i++)
{
scanf("%d",&m[i]);
printf("%d")}
max=m[0];
min=m[0];
for(i=1;i<=4;i++)
{
    if(max<=m[i])
    {
        max=m[i];
    }
    if(min>=m[1])
    {
        min=m[i];
    }
}
printf("the maximum of the numbers is %d\n",max);
printf("the minimum of the numbers is %d\n",min);

for(i=1;i<=4;i++)
{
    if ( m[i]>=40)
    {
        
        printf("the student is passed\n");
    }
    else 
    {
        printf("the student is failed\n");
    }
}
return 0;
}
